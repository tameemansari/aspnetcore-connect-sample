@using Microsoft.AspNetCore.Http
<h2>Loading converter!</h2>
@if (User.Identity.IsAuthenticated)
{
    <div class="alert alert-info @(Context.Session.GetString("SSAccessToken") == null ? "hidden" : null)" role="alert">
        @Html.Raw(Context.Session.GetString("SSAccessToken"))
    </div>
}